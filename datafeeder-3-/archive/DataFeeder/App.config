<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <!--NOTE : SymbolList ==>> It contains pair of symbol and security type. Each pair should be comma separated, EX: EURUSD_FOREX,GOLD_FUTURE.
    DupSymbolList contains pair of original symbol(symbols subscribed to MT4) and duplicate symbol name, we can duplicate different symbols but each 
    pair should be comma separated. Ex EURUSD_DUP1,GOLD_DUP2-->
    <!--<add key="SymbolList" value="EURUSD_FOREX,USDJPY_FOREX" />
    <add key="DupSymbolList" value="" />-->
    <add key="MT4SymbolList" value="EURUSD,AUDCAD,AUDCHF,AUDJPY,AUDNZD,AUDUSD,CADCHF,CADJPY,CHFJPY,EURAUD,EURCAD,EURCHF,EURGBP,EURJPY,EURNZD,GBPCHF,GBPJPY,GBPUSD,GOLD,NZDJPY,NZDUSD,USDCAD,USDCHF,USDJPY,XAGUSD,XAUUSD" />
